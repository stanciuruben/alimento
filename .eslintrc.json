{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": "standard-with-typescript",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": ["./tsconfig.json", "./tsconfig.eslint.json"]
	},
	"rules": {
		"semi": "off",
		"@typescript-eslint/semi": "off",
		"indent": "off",
		"@typescript-eslint/indent": "off",
		"no-tabs": "off",
		"@typescript-exlint/no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"@typescript-exlint/no-mixed-spaces-and-tabs": "off"
	}
}
